            <table class="table table-dark">
                <thead class="thead-dark">
                <tr> 
                <th>ID</th>
                <th>Title</th>
                <th>Author</th>
                <th>Price</th>
                <th>Image</th>
        
            </tr>
        </thead>
        <tbody>
         <%   products.forEach((product)=>{ %>

            
<tr>
    <td><%= product.id %></td>
    <td><%= product.title %></td>
    <td><%= product.author %></td>
    <td><%= product.price %></td>
    <td><img src="<%= product.img %>" /></td>
    
 </tr> 
<% }) %>

</tbody>
            </table>
    